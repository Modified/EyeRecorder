{
	"name":"my plugin",
	"version":"0",
	"description":"What do I do as an extension",
	"background":{
		"scripts":["background.js"]
	},
	"manifest_version":2,
	"browser_action":{
		"name":"Manipulate DOM",
		"icons":["icon.png"],
		"default_icon":"icon.png",
		"default_popup":"popup.html"
	},
	"content_scripts":[{
		"js":["content_script.js"],
		"css":["styling.css"],
		"matches":["http://*/*", "https://*/*"]
	}],
	"permissions":[
		"storage"
	]
}
