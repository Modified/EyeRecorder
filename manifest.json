{
  "name": "my plugin",
  "version": "0",
  "description": "What do I do as an extension",
  "background": {
      "scripts" : ["background.js"]
  },
  "manifest_version": 2,
  "browser_action": {
    "name": "Manipulate DOM",
    "icons": ["icon.png"],
    "default_icon": "icon.png",
    "default_popup" : "popup.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-2.0.2.min.js", "content_script.js" ],
    "css": ["customStyles.css"],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
